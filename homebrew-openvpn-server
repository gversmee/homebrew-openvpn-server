class OpenvpnServer < Formula
  desc ""
  homepage "https://github.com/gversmee/openvpn-server"
  url "https://github.com/gversmee/openvpn-server/archive/v0.91.tar.gz"
  sha256 "d801d6ef699ac0186cfdc6c7b1c611dbd5cab8986f202e50f954fd8005e989db"

  def install
    bin.install "vpn-server-start"
    bin.install "vpn-server-stop"
  end

end
